<h1> <%= @contact.name %> </h1>

<h3> <%= @contact.phone %> </h3>

<h3> <%= @contact.email %> </h3>

<ul>
  <li><%= link_to "Edit", edit_contact_path(@contact) %></li>
  <li><%= link_to "Destroy", contact_path(@contact),
                  method: :delete,
                  data: { confirm: "Are you sure?" } %></li>
</ul>
  

<p><br>Find me in app/views/contacts/show.html.erb</p>
