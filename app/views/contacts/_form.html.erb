<%= form_with model: contact do |form| %>
  <div>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :phone %><br>
    <%= form.text_field :phone %><br>
  </div>

    <div>
    <%= form.label :email %><br>
    <%= form.text_field :email %><br>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
